ede_FILES=Project.ede Makefile

eieio_LISP=eieio.el eieio-custom.el eieio-opt.el eieio-comp.el eieio-doc.el
EMACS=emacs
eieio_info_INFOS=eieio.texi
Misc_AUX=ChangeLog
examples_LISP=tree.el chart.el eieio-speedbar.el eieio-tests.el
EMACS=emacs
VERSION=$(shell grep "Version: " eieio.el | cut -d" " -f3)
DISTDIR=eieio-$(VERSION)



all: eieio eieio.info examples

eieio:
	@echo "(add-to-list 'load-path \"$(PWD)\")" > eieio-compile-script
	@echo "(require 'eieio)" >> eieio-compile-script
	@echo "(require 'eieio-comp)" >> eieio-compile-script
	@echo "(require 'eieio-custom)" >> eieio-compile-script
	$(EMACS) -batch -l eieio-compile-script -f batch-byte-compile  $(eieio_LISP)

eieio.info: $(eieio_info_INFOS)
	makeinfo eieio.texi
examples:
	@echo "(add-to-list 'load-path \"$(PWD)\")" > examples-compile-script
	@echo "(require 'cl)" >> examples-compile-script
	@echo "(require 'eieio-speedbar)" >> examples-compile-script
	@echo "(require 'tree)" >> examples-compile-script
	@echo "(require 'chart)" >> examples-compile-script
	$(EMACS) -batch -l examples-compile-script -f batch-byte-compile  $(examples_LISP)

clean:
	rm -f *.elc *.info


dist:
	rm -rf $(DISTDIR)
	mkdir $(DISTDIR)
	cp $(eieio_LISP) $(eieio_info_INFOS) $(Misc_AUX) $(examples_LISP) $(ede_FILES) $(DISTDIR)
	tar -cvzf $(DISTDIR).tar.gz $(DISTDIR)
	rm -rf $(DISTDIR)

tags: $(eieio_LISP) $(eieio_info_INFOS) $(examples_LISP) 
	etags $^

Makefile: Project.ede
	@echo Makefile is out of date!  It needs to be regenerated by EDE.
	@false



# End of Makefile
