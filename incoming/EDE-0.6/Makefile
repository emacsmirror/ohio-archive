ede_FILES=Project.ede Makefile

aux_AUX=NEWS README TODO
ede_info_INFOS=ede.texi ede-projects.texi ede-targets.texi
ede_LISP=ede.el ede-proj.el ede-speedbar.el project-am.el ede-pmake.el ede-pconf.el ede-dired.el
EMACS=emacs
LOADPATH=~/lisp/eieio ~/lisp/speedbar
DISTDIR=EDE-0.6



all: ede ede.info

ede:
	@echo "(add-to-list 'load-path \"$(PWD)\")" > ede-comp
	@for loadpath in ${LOADPATH}; do \
	  echo "(add-to-list 'load-path \"$$loadpath\")" >> ede-comp; \
	  done
	@echo "(require 'ede)" >> ede-comp
	@echo "(require 'ede-proj)" >> ede-comp
	@echo "(require 'ede-pmake)" >> ede-comp
	@echo "(require 'project-am)" >> ede-comp
	@echo "(require 'ede-speedbar)" >> ede-comp
	$(EMACS) -batch -l ede-comp -f batch-byte-compile $(ede_LISP)

ede.info: $(ede_info_INFOS)
	makeinfo ede.texi

clean:
	rm -f *.elc *.info


dist:
	rm -rf $(DISTDIR)
	mkdir $(DISTDIR)
	cp $(aux_AUX) $(ede_info_INFOS) $(ede_LISP) $(ede_FILES) $(DISTDIR)
	tar -cvzf $(DISTDIR).tar.gz $(DISTDIR)
	rm -rf $(DISTDIR)

Makefile: Project.ede
	@echo Makefile is out of date!  It needs to be regenerated by EDE.
	@false



