ede_FILES=Project.ede Makefile

tools_LISP=document.el document-vars.el sformat.el semantic-make.el semantic-c.el semantic-imenu.el semantic-el.el semantic-sb.el working.el
EMACS=emacs
info_INFOS=semantic.texi
aux_AUX=c.bnf make.bnf
semantic_LISP=semantic-bnf.el semantic.el semantic-util.el
EMACS=emacs
VERSION=1.2
DISTDIR=semantic-$(VERSION)



all: tools info semantic

tools:
	@echo "(add-to-list 'load-path \"$(PWD)\")" > tools-compile-script
	@echo "(setq debug-on-error t)" >> tools-compile-script
	$(EMACS) -batch -l tools-compile-script -f batch-byte-compile  $(tools_LISP)

info: $(info_INFOS)
	makeinfo semantic.texi
semantic:
	@echo "(add-to-list 'load-path \"$(PWD)\")" > semantic-compile-script
	@echo "(setq debug-on-error t)" >> semantic-compile-script
	$(EMACS) -batch -l semantic-compile-script -f batch-byte-compile  $(semantic_LISP)

clean:
	rm -f *.elc *.info


dist:
	rm -rf $(DISTDIR)
	mkdir $(DISTDIR)
	cp $(tools_LISP) $(info_INFOS) $(aux_AUX) $(semantic_LISP) $(ede_FILES) $(DISTDIR)
	tar -cvzf $(DISTDIR).tar.gz $(DISTDIR)
	rm -rf $(DISTDIR)

tags: $(tools_LISP) $(info_INFOS) $(semantic_LISP) 
	etags $^

Makefile: Project.ede
	@echo Makefile is out of date!  It needs to be regenerated by EDE.
	@false



